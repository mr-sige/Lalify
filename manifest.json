{
  "manifest_version": 2,

  "name": "Lalify",
  "description": "Lalify",
  "version": "1.0",

"content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["lalify.js"],
      "css": ["lalify.css"]
    }
  ],

  "browser_action": {
  },

  "permissions": [
    "activeTab",
    "*://*/*"
  ],

  "web_accessible_resources": ["lalify.css", "lali.png"]
}